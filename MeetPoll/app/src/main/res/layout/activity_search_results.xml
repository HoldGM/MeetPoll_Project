<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res-auto"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android1:layout_height="wrap_content"
    android1:layout_width="match_parent">
    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="odb234.meetpoll.SearchResultsActivity" />
    <Switch
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:id="@+id/map_list_select"
        android1:checked="false"
        android1:textOff="Map"
        android1:textOn="List"
        android1:scaleX="1.25"
        android1:scaleY="1.25"
        android1:layout_alignParentTop="true"
        android1:layout_alignParentRight="true"
        android1:layout_alignParentEnd="false"
        android1:layout_marginRight="20dp"
        android1:layout_marginTop="15dp"/>

</RelativeLayout>
